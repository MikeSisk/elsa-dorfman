%nav
  %div{:id => "category_nav", :class => "#{@category_name}"}
    #line
    %ul{:id => "line_list"}
      - @links.each do |link|
        = content_tag_for :li, link.resource do
          =link_to link.label, link.dest, :class => (link.selected ? 'selected' : '')


